{
  "name": "nod",
  "version": "0.0.0",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "start-dev-frontend": "cd frontend && ng serve",
    "start-dev-backend": "cd backend && npm run start:watch",
    "start-dev": "npm run start-dev-frontend & npm run start-dev-backend",
    "build-backend": "cd backend && npm run build",
    "build-frontend": "cd frontend && ng build --aot --prod",
    "build-frontend-no-prod": "cd frontend && ng build --aot",
    "build": "npm run build-frontend && npm run build-backend",
    "build-no-prod": "npm run build-frontend-no-prod && npm run build-backend",
    "start": "pm2-runtime start ecosystem.config.js --env production"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/animations": "~9.1.3",
    "@angular/cli": "~9.1.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/forms": "~9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@angular/service-worker": "~9.1.3",
    "@types/socket.io": "^2.1.4",
    "@types/ws": "^7.2.4",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "heroku": "^7.39.5",
    "immutable": "^4.0.0-rc.12",
    "log4js": "^6.1.2",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "ngx-device-detector": "^1.4.6",
    "ngx-socket-io": "^3.0.1",
    "pm2": "^4.4.0",
    "request": "^2.88.0",
    "rxjs": "~6.5.4",
    "socket.io": "^2.3.0",
    "ts-node": "^8.8.2",
    "tslib": "^1.10.0",
    "typescript": "3.8.3",
    "ws": "^7.2.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^1.19.4",
    "tslint": "^6.1.1"
  }
}

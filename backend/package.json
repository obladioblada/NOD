{
  "name": "nod-server",
  "version": "0.0.0",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "private": true,
  "dependencies": {
    "@types/mongoose": "^5.7.14",
    "compression": "^1.7.4",
    "request": "^2.88.0",
    "rxjs": "^6.5.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "ws": "^7.2.3",
    "@types/socket.io": "^2.1.8",
    "@types/ws": "^7.2.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pm2": "^4.4.0",
    "socket.io": "^2.3.0",
    "tslib": "^1.13.0",
    "log4js": "^6.3.0",
    "mongoose": "^5.9.25"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^13.13.14",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^1.19.4",
    "tslint": "^6.1.1"
  },
  "allowSyntheticDefaultImports": true,
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}

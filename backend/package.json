{
  "name": "nod-server",
  "version": "0.0.0",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "private": true,
  "dependencies": {
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "express": "^4.17.1",
    "nodemon": "^1.19.4",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
